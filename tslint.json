{
  "extends": [
    "tslint:recommended",
    "tslint-config-standard"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": false,
    "max-classes-per-file": false,
    "max-line-length": [true, 300],
    "no-conditional-assignment": false,
    "no-console": false,
    "triple-equals": [true, "allow-null-check"],
    "unified-signatures": false
  }
}
